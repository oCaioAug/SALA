{
  "expo": {
    "name": "SALA",
    "slug": "sala-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3B82F6"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "dev.ocaioaug.salamobile"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "webClientId": "199539496823-sgm07ecndbi76o04c4d95ogg381onmdr.apps.googleusercontent.com",
          "iosUrlScheme": "com.googleusercontent.apps.199539496823-nbechbht00i7cv6gc99df5f6p0coq6vp"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "A aplicação precisa acessar suas fotos para permitir a alteração do avatar.",
          "cameraPermission": "A aplicação precisa acessar a câmera para permitir a captura de fotos para o avatar.",
          "microphonePermission": false
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "googleSignIn": {
        "webClientId": "199539496823-sgm07ecndbi76o04c4d95ogg381onmdr.apps.googleusercontent.com"
      },
      "eas": {
        "projectId": "8c01597d-b32d-4d6e-96aa-3a91b739a7e7"
      }
    },
    "scheme": "sala",
    "owner": "ocaioaug"
  }
}
